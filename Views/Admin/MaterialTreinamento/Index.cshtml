@{
    ViewBag.Title = "AlphaBook - Administração";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

@section scripts {
    <script type="text/javascript" src="@Url.Content("~/Scripts/Admin/Material/material-pesquisa.js")"></script>
}

@section css {
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/Admin/material-treinamento.css")" />
}

<h2>Cadastro de Material de Treinamento</h2>

<div class="div-acoes">
	<a id="incluir" class="bt-adicionar" href="@Url.Action("EditarMaterial", "Admin")">Adicionar Novo Material</a>
</div>

<div class="div-listar">
    <div class="box-tabela">
        <div class="wrap table2">
            <div class="inner">
                <table id="tabela">
                    <thead>
                        <tr>
                            <th class="table-title table-left col-1 table-title-ord-down">
                                <a href="javascript:void(0);" data-ordenacao="1" data-tipo="1">Título</a>
                            </th>
                            <th class="table-title col-2">
                                Download
                            </th>
                            <th class="table-title col-3">
                                Excluir
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>