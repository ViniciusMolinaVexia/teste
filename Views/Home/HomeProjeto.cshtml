@model Gafisa.AlphaBook.Dominio.DTO.ProjetoDto

<div data-idprojeto="0" data-blocoexibido="0" class="carrossel" id="carrosselProjetos" style="opacity: 1; width: 100%;"> 
    <div class="hide loading-circulo loading-carrossel" id="carregandoCarrossel"></div>
    <div id="section1" class="section-project" style="width: 100%;">
        <div data-id="2" class="tile-projeto" style="width: 100%;">
            <div class="tile-topo">
                <h2 class="">@Model.Nome</h2>
                <h3>@string.Format("{0} - {1}", Model.Cidade, Model.Estado)</h3>
            </div>
        </div>
    </div>
</div>